---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>Nicolas Garcia Belmonte: Data visualizations</title>
 <link href="http://philogb.github.io/feed.xml" rel="self"/>
 <link href="https://philogb.github.io/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://philogb.github.io/</id>
 <author>
   <name>Nicolas Garcia Belmonte</name>
   <email>philogb@gmail.com</email>
 </author>
 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   {% if post.fullurl %}
    <link href="{{ post.fullurl }}"/>
   {% else %}
    <link href="{{ post.url }}"/>
   {% endif %}
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   {% if post.fullurl %}
   <id>{{ post.fullurl }}</id>
   {% else %}
   <id>{{ post.url }}</id>
   {% endif %}
   <content type="html">
    {% if post.content != '' %}
      {{ post.content | xml_escape }}
    {% else %}
    &lt;img src="{{ post.thumb }}" />
    {% endif %}
   </content>
 </entry>
 {% endfor %}

</feed>

